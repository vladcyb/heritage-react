{
  "compilerOptions": {
    "baseUrl": "./src",
    "module": "ES2020",
    "paths": {
      "@app/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@pages/*": ["src/pages/*"],
      "@assets/*": ["src/assets/*"],
      "@services/*": ["src/services/*"],
      "@shared/*": ["src/shared/*"],
      "@slices/*": ["src/slices/*"],
      "@enums/*": ["src/enums/*"],
      "@models/*": ["src/models/*"]
    },
    "moduleResolution": "Node"
  }
}